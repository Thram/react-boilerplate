{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for React applications",
  "main": "dist/index.js",
  "repository": "https://github.com/Thram/react-boilerplate",
  "author": "Thram <thramposo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "setup": "yarn install && node ./setup.js",
    "start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "prebuild": "rimraf release",
    "build": "NODE_ENV=production webpack -p",
    "preqa": "rimraf qa",
    "analyze": "yarn qa --env.analyze",
    "qa": "NODE_ENV=development webpack --env.qa",
    "precz": "yarn test",
    "cz": "git add . && git cz",
    "lint": "eslint src --ext .jsx,.js",
    "lint:test": "eslint __test__ --ext .js",
    "test": "yarn lint && jest",
    "snap": "yarn test -u",
    "storybook": "start-storybook -p 6006",
    "docs": "build-storybook -o ${PWD}/docs"
  },
  "jest": {
    "transformIgnorePatterns": ["node_modules/(?!(redux-persist/es/storage))"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|yml|yaml)$":
        "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss|sass|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-info": "^3.3.10",
    "@storybook/addon-knobs": "^3.3.10",
    "@storybook/addon-options": "^3.3.10",
    "@storybook/addon-storyshots": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-emotion": "8.0.12",
    "babel-plugin-react-element-info": "^1.0.1",
    "babel-plugin-transform-remove-console": "6.8.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "commitizen": "^2.9.6",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.9",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "5.0.1",
    "jest": "^22.1.4",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.2",
    "preload-webpack-plugin": "^2.2.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "storybook-addon-jsx": "^5.3.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.1",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "emotion": "8.0.12",
    "lodash": "^4.17.4",
    "process-reducer": "1.0.3",
    "prop-types": "^15.6.0",
    "purecss": "1.0.0",
    "rc-hammerjs": "0.6.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "8.0.12",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.5.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
